<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.projectakhir.juanlazuardo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".projectakhir.WalkthroughActivity"></activity>
        <activity android:name=".projectakhir.RegisterActivity"></activity>
        <activity android:name=".projectakhir.LoginActivity" />
        <activity
            android:name=".projectakhir.SplashActivity"
            android:theme="@style/SplashScreenStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".projectakhir.MainActivity" />
    </application>

</manifest>